      /*%METADATA                                                     */
      /* %TEXT Transform num ISO to date, 0 = 01/01/1940              */
      /*%EMETADATA                                                    */
     CREATE FUNCTION ISOTODATE40 (
       DECIMAL(8, 0) )
       RETURNS DATE
       LANGUAGE RPGLE
       SPECIFIC ISOTODATE4
       DETERMINISTIC
       NO SQL
       RETURNS NULL ON NULL INPUT
       NO EXTERNAL ACTION
       EXTERNAL NAME DAT002
       PARAMETER STYLE SQL ;
